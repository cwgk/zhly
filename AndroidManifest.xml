<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cwgk.zhly"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />
    <!-- android:theme="@android:style/Theme.Wallpaper.NoTitleBar.Fullscreen" -->
    <application
        android:name="com.cwgk.util.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Wallpaper.NoTitleBar" >
        <activity android:name="com.cwgk.zhly.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cwgk.zhly.MainActivity" 
           >
            <intent-filter>
                <action android:name="com.cwgk.zhly.MAIN_VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.cwgk.zhly.SecondActivity" >
        </activity>
        <activity android:name="com.cwgk.zhly.ContentActivity" >
        </activity>
        <activity android:name="com.cwgk.zhly.SettingActivity"
            >
        </activity>
    </application>

</manifest>